#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    int n;
    cin>>n;
    int x, count[100];
    for(int i = 0; i < 100; i++) {
        count[i] = 0;
    }
    for(int i = 0; i < n; i++) {
        int x;
        cin>>x;
        count[x]++;
    }
    for(int i = 0; i < 100; i++) {
        cout<<count[i]<<" ";
    }
    return 0;
}
