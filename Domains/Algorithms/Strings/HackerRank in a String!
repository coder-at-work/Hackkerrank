#include <bits/stdc++.h>

using namespace std;

int main(){
    int q;
    cin >> q;
    for(int a0 = 0; a0 < q; a0++){
        string s, check = "hackerrank";
        cin >> s;
        int j = 0, length = 10;
        for(int i = 0; i < s.length(); i++) {
            if(check[j] == s.at(i)) {
                j++;
            }
        }
        if(j == 10)
            cout<<"YES"<<endl;
        else
            cout<<"NO"<<endl;
    }
    return 0;
}
