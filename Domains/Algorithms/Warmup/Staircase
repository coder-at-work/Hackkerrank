#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main(){
    int n, sp;
    cin >> n;
    sp = n;
    for(int i=0;i<n;i++)
    {
        for(int j=0;j<sp-1;j++)
            cout<<" ";
        for(int k=0;k<=i;k++)
            cout<<"#";
        cout<<endl;
        sp--;
    }
    return 0;
}
