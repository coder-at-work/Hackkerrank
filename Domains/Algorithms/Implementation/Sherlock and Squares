#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    int n;
    cin>>n;
    while(n--) {
        int a, b;
        cin>>a>>b;
        int ans = floor(sqrt(b)) - ceil(sqrt(a)) + 1;
        cout<<ans<<endl;
    }
    return 0;
}
